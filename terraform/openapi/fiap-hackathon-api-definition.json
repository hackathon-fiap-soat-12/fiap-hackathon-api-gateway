          "properties": {
            "id": {
              "type": "string",
              "format": "uuid",
              "example": "031193eb-6464-4d18-91f7-d0be91714561"
            },
            "videoName": {
              "type": "string",
              "example": "video.mp4"
            },
            "processingStatus": {
              "type": "string",
              "enum": ["NEW", "RECEIVED", "PROCESSING", "PROCESSED", "FAILED"],
              "example": "NEW"
            }
          }
        },
        "ProblemDto": {
          "properties": {
            "message": {
              "type": "string",
              "example": "Error Message"
            },
            "dateTime": {
              "type": "string",
              "example": "2025-03-31T16:46:34.792653300"
            }
